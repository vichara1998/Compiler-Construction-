- Follow(prog) =                       { Begin }
- Follow(codeBlock) =                  { Begin }
- Follow(classOrImplOrFunc) =          { First(codeBlock) except ϵ union Follow(codeBlock) }
- Follow(classDecl) =                  { Follow(classOrImplOrFunc) }
- Follow(implDef) =                    { Follow(classOrImplOrFunc) }
- Follow(memberDecl) =                 { First(classBody) except ϵ union Follow(classBody) }
- Follow(funcDecl) =                   { Follow(memberDecl) }
- Follow(implBody) =                   { } }
- Follow(funcDef) =                    { Follow(classOrImplOrFunc) union First(implBody) except ϵ union Follow(implBody)}
- Follow(funcHead) =                   { First(funcBody) except ϵ union ; }
- Follow(classBody) =                  { } }
- Follow(visibility) =                 { First(memberDecl) except ϵ }
- Follow(attributeDecl) =              { Follow(memberDecl) }
- Follow(funcBody) =                   { Follow(funcDef) }
- Follow(funcContents) =               { } }
- Follow(varDeclOrStmt) =              { First(funcContents) except ϵ union Follow(funcContents) }
- Follow(localVarDecl) =               { Follow(varDeclOrStmt) }
- Follow(statBlock) =                  { else , ; }
- Follow(aParams)  =                   { ) }
- Follow(expr) =                       { ) ,  Follow(assignStat) union First(aParamsCont) except ϵ union Follow(aParams) union Follow(aParamsTail) }
- Follow(relExpr) =                    { ) , Follow(expr) }
- Follow(arithExpr) =                  { Follow(expr) union First(relOp) except ϵ union Follow(relExpr) , ) , ] }
- Follow(term) =                       { First(arithOperations) except ϵ union Follow(arithExpr) union Follow(arithOperations)  }
- Follow(factor) =                     { First(termOperations) except ϵ union Follow(term) union Follow(termOperations) }
- Follow(sign) =                       { First(factor) except ϵ }
- Follow(statBlockContents) =          { } }
- Follow(statement) =                  { Follow(varDeclOrStmt) union Follow(statBlock) union First(statBlockContents) except ϵ union Follow(statBlockContents) }
- Follow(functionCall) =               { ; , Follow(factor) }
- Follow(variable) =                   { ) , First(assignOp) except ϵ union Follow(factor) }
- Follow(prefix) =                     { id }
- Follow(assignStat) =                 { ;  }
- Follow(idnest) =                     { First(prefix) except ϵ union Follow(prefix) }
- Follow(idOrSelf) =                   { First(identifier .) except ϵ , ( }
- Follow(identifier)  =                { . }
- Follow(varSelector)  =               { Follow(variable) }
- Follow(indice)  =                    { First(identifier) except ϵ union Follow(identifier) union First(varSelector) except ϵ union Follow(varSelector) }
- Follow(arrLength) =                  { ; , First(fParamsCont) except ϵ union Follow(fParams) union Follow(fParamsTail) }
- Follow(arraySize)  =                 { First(arrLength) except ϵ union Follow(arrLength) }
- Follow(returnType)  =                { Follow(funcHead) }
- Follow(type)  =                      { First(arrLength) except ϵ union Follow(returnType) First(arrLength fParamsCont) except ϵ union Follow(fParams) union First(arrLength) except ϵ union Follow(fParamsTail) ; }
- Follow(fParams)  =                   { ) }
- Follow(fParamsCont) =                { Follow(fParams) }
- Follow(fParamsTail)  =               { First(fParamsCont) except ϵ union Follow(fParamsCont) }
- Follow(aParamsCont) =                { Follow(aParams) }
- Follow(aParamsTail)  =               { First(aParamsCont) except ϵ union Follow(aParamsCont) }
- Follow(assignOp)  =                  { First(expr) except ϵ }
- Follow(relOp)  =                     { First(arithExpr) except ϵ }
- Follow(arithOperations) =            { Follow(arithExpr) }
- Follow(addOp)  =                     { First(term) }
- Follow(termOperations) =             { Follow(term) }
- Follow(multOp)  =                    { First(factor) except ϵ }
- Follow(varDecl)  =                   { Follow(attributeDecl) union Follow(localVarDecl)}
- Follow(interface) =                  { Follow(classExtension) }
- Follow(classExtension) =             { { }
